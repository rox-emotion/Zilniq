{
  "expo": {
    "name": "Zilniq",
    "slug": "Zilniq",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/default.png",
    "scheme": "zilniq",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.emotionstudios.zilniq",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserNotificationsUsageDescription": "This app uses notifications to send you personalized health and nutrition updates."
      },
      "icon": {
        "light": "./assets/default.png",
        "dark": "./assets/dark.png",
        "tinted": "./assets/tinted-dark.png"
      }
    },
    "android": {
      "icon": "./assets/default.png",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.emotionstudios.zilniq"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      "expo-video",
      [
        "expo-notifications",
        {
          "icon": "./assets/default.png",
          "color": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ae74d5f4-3901-4bc5-8214-1247046e1f11"
      }
    }
  }
}
